Cell["\<\
First I defined a module to compute the Three-Eighths Simpson\
\[CloseCurlyQuote]s Rule over a single interval.\
\>", "Text",
 CellChangeTimes->{{3.6249176915747457`*^9, 3.624917716176419*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"TESimp", "[", 
   RowBox[{"f_", ",", "a_", ",", "b_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"x0", "=", "a"}], ",", "x1", ",", "x2", ",", 
      RowBox[{"x3", "=", "b"}], ",", 
      RowBox[{"h", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"b", "-", "a"}], ")"}], "/", "3"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"x1", "=", 
      RowBox[{"x0", "+", "h"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"x2", "=", 
      RowBox[{"x0", "+", 
       RowBox[{"2", "*", "h"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"N", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"3", "*", 
         RowBox[{"h", "/", "8"}]}], ")"}], "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"f", "[", "x0", "]"}], "+", 
         RowBox[{"3", "*", 
          RowBox[{"f", "[", "x1", "]"}]}], "+", 
         RowBox[{"3", "*", 
          RowBox[{"f", "[", "x2", "]"}]}], "+", 
         RowBox[{"f", "[", "x3", "]"}]}], ")"}]}], "\[IndentingNewLine]", 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.624917718266859*^9, 3.6249179492839355`*^9}}],

