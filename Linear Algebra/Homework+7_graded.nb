Cell[CellGroupData[{

Cell[TextData[{
 StyleBox["Use theorem 4.2 to derive ``Simpson\[CloseCurlyQuote]s \
Three-Eighths Rule\[CloseCurlyQuote]\[CloseCurlyQuote], recall that this is \
the name given to the closed Newton-Cotes formula when ", "Text",
  FontSize->16],
 Cell[BoxData[
  FormBox[
   RowBox[{"n", " ", "=", " ", "3"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 StyleBox[".\n", "Text",
  FontSize->16]
}], "Subsubsection",
 CellChangeTimes->{{3.6200581944663525`*^9, 3.6200582295288587`*^9}, 
   3.620058339494854*^9, {3.6200862901647587`*^9, 3.6200863543835187`*^9}, {
   3.6205890390910788`*^9, 3.6205890684060097`*^9}, {3.6205891492450933`*^9, 
   3.620589192627431*^9}, {3.620589318819049*^9, 3.6205893401871853`*^9}, {
   3.6218500443560143`*^9, 3.6218500665492334`*^9}, {3.621850134854063*^9, 
   3.6218501454281206`*^9}, 3.622380406633805*^9, {3.6223807463064384`*^9, 
   3.6223808859614673`*^9}, {3.6224028974328394`*^9, 3.622402900537339*^9}, {
   3.6249138420568867`*^9, 3.624913899863802*^9}, {3.6249140523999834`*^9, 
   3.624914098964769*^9}, {3.6249142175529704`*^9, 3.624914352989866*^9}, {
   3.624914760396517*^9, 3.624914801938049*^9}, {3.624920821635665*^9, 
   3.624920822743272*^9}}],

Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", " ", "=", " ", "3"}], TraditionalForm]]],
 ". Theorem 4.2 says that (when ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]]],
 " is odd),\n      ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "a", "b"], 
     RowBox[{
      RowBox[{"f", "(", "x", ")"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}], "n"], 
      RowBox[{
       SubscriptBox["a", "i"], 
       RowBox[{"f", "(", 
        SubscriptBox["x", "i"], ")"}]}]}], " ", "+", " ", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["h", 
         RowBox[{"n", "+", "2"}]], 
        RowBox[{
         SuperscriptBox["f", 
          RowBox[{"(", 
           RowBox[{"n", "+", "1"}], ")"}]], "(", "\[Xi]", ")"}]}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"n", "+", "1"}], ")"}], "!"}]], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "n"], 
       RowBox[{
        RowBox[{"t", "(", 
         RowBox[{"t", "-", "1"}], ")"}], 
        RowBox[{"\[CenterEllipsis]", "(", 
         RowBox[{"t", "-", "n"}], ")"}], 
        RowBox[{"\[DifferentialD]", "t"}]}]}]}]}]}], TraditionalForm]]],
 "\nfor some ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Xi]", "\[Element]", 
    RowBox[{"(", 
     RowBox[{"a", ",", "b"}], ")"}]}], TraditionalForm]]],
 " where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["a", "i"], " ", "=", " ", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "a", "b"], 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{
         RowBox[{"j", "=", "0"}], ",", 
         RowBox[{"j", "\[NotEqual]", "i"}]}], "n"], 
       RowBox[{
        FractionBox[
         RowBox[{"(", 
          RowBox[{"x", "-", 
           SubscriptBox["x", "j"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["x", "i"], "-", 
           SubscriptBox["x", "j"]}], ")"}]], 
        RowBox[{
         RowBox[{"\[DifferentialD]", "x"}], "."}]}]}]}]}]}], 
   TraditionalForm]]],
 "\nFirst let\[CloseCurlyQuote]s compute the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "i"], TraditionalForm]]],
 ".\n       ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "0"], " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", "a", "b"], 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x", "-", 
           SubscriptBox["x", "1"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{"x", "-", 
           SubscriptBox["x", "2"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{"x", "-", 
           SubscriptBox["x", "3"]}], ")"}]}], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["x", "1"], "-", 
           SubscriptBox["x", "2"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["x", "1"], "-", 
           SubscriptBox["x", "3"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["x", "1"], "-", 
           SubscriptBox["x", "4"]}], ")"}]}]], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "=", " ", 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Integral]", "a", "b"], 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["x", "3"], "-", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             SubscriptBox["x", "1"], "+", 
             SubscriptBox["x", "2"], "+", 
             SubscriptBox["x", "3"]}], ")"}], 
           SuperscriptBox["x", "2"]}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              SubscriptBox["x", "1"], 
              SubscriptBox["x", "2"]}], "+", 
             RowBox[{
              SubscriptBox["x", "1"], 
              SubscriptBox["x", "3"]}], "+", 
             RowBox[{
              SubscriptBox["x", "2"], 
              SubscriptBox["x", "3"]}]}], ")"}], "x"}], "-", 
          RowBox[{
           SubscriptBox["x", "1"], 
           SubscriptBox["x", "2"], 
           SubscriptBox["x", "3"]}]}], 
         RowBox[{
          RowBox[{"h", "(", 
           RowBox[{"2", "h"}], ")"}], 
          RowBox[{"(", 
           RowBox[{"3", "h"}], ")"}]}]], 
        RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "=", " ", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"6", 
         SuperscriptBox["h", "3"]}]], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "a", "b"], 
        RowBox[{
