Cell[BoxData["3.`"], "Output",
 CellChangeTimes->{3.6252399335161424`*^9}]
}, Open  ]],

Cell[TextData[{
 "By a theorem from the other day we can compute the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["l", "2"], TraditionalForm]]],
 " norm as follows."
}], "Text",
 CellChangeTimes->{{3.624927248664305*^9, 3.624927272743713*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"l2", "[", "A_", "]"}], ":=", 
   RowBox[{"N", "[", 
    RowBox[{"Sqrt", "[", 
     RowBox[{"SpectralRadius", "[", 
      RowBox[{
       RowBox[{"Transpose", "[", "A", "]"}], ".", "A"}], "]"}], "]"}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.624927222640703*^9, 3.624927238672306*^9}}],

